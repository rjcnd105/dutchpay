{
  "name": "remix-template-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm run generate:css",
    "build:prisma-gen": "prisma generate",
    "build:remix": "remix build",
    "dev": "PORT=3300 run-p dev:*",
    "dev:css": "pnpm run generate:css -w",
    "dev:remix": "remix dev",
    "dev:prisma-gen": "prisma generate --watch",
    "generate:css": "postcss ./styles/**/*.css styles --dir ./app/styles",
    "start": "remix-serve build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run:flow": "./node_modules/.bin/cypress run --spec cypress/integration/e2e/$FLOW/_index.spec.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "dbpush": "prisma db push",
    "dbpull": "prisma db pull --print",
    "dbstudio": "prisma studio",
    "gen-icons": "pnpx @svgr/cli --out-dir app/components/ui/icon --typescript -- public/icons && sudo chown \"$(whoami)\" app/components/ui/Icon",
    "tailwind-viewer": "pnpx tailwind-config-viewer -o",
    "migrate:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts",
    "deleteEmptyPayItems": "node --require esbuild-register prisma/deleteEmptyPayItems.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/api"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@notionhq/client": "^2.2.2",
    "@prisma/client": "^4.7.1",
    "@remix-run/node": "^1.8.0",
    "@remix-run/react": "^1.8.0",
    "@remix-run/server-runtime": "^1.8.0",
    "@remix-run/vercel": "^1.8.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vercel/node": "^2.7.1",
    "@zag-js/checkbox": "^0.2.3",
    "@zag-js/react": "^0.3.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.13.1",
    "framer-motion": "^7.6.18",
    "immer": "^9.0.16",
    "io-ts": "^2.2.20",
    "rc-drawer": "4.4.3",
    "rc-input-number": "^7.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.4",
    "remix-utils": "^4.3.0",
    "superjson": "^1.11.0",
    "use-debounce": "^9.0.2",
    "use-immer": "^0.8.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@remix-run/dev": "^1.8.0",
    "@remix-run/eslint-config": "^1.8.0",
    "@remix-run/serve": "^1.8.0",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cssnano": "^5.1.14",
    "cypress": "^11.2.0",
    "esbuild": "^0.15.17",
    "esbuild-register": "^3.4.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "react-toastify": "^9.1.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3"
  },
  "engines": {
    "node": ">=18"
  }
}
