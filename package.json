{
  "name": "remix-template-vercel",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm run generate:css",
    "build:prisma-gen": "prisma generate",
    "build:remix": "remix build",
    "dev": "PORT=3300 run-p dev:*",
    "dev:css": "pnpm run generate:css -w",
    "dev:remix": "remix dev",
    "dev:prisma-gen": "prisma generate --watch",
    "generate:css": "postcss ./styles/**/*.css styles --dir ./app/styles",
    "start": "remix-serve build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run:flow": "./node_modules/.bin/cypress run --spec cypress/integration/e2e/$FLOW/_index.spec.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "dbpush": "prisma db push",
    "dbpull": "prisma db pull --print",
    "dbstudio": "prisma studio",
    "gen-icons": "pnpx @svgr/cli --out-dir app/components/ui/icon --typescript -- public/icons && sudo chown \"$(whoami)\" app/components/ui/Icon",
    "tailwind-viewer": "pnpx tailwind-config-viewer -o",
    "migrate:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts",
    "deleteEmptyPayItems": "node --require esbuild-register prisma/deleteEmptyPayItems.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/api"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.8",
    "@notionhq/client": "^2.2.3",
    "@prisma/client": "^4.9.0",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@remix-run/css-bundle": "^1.11.1",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/server-runtime": "^1.11.1",
    "@remix-run/vercel": "^1.11.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vercel/node": "^2.8.16",
    "@effect/io": "^0.1.3",
    "@fp-ts/core": "^0.1.1",
    "@fp-ts/data": "^0.1.0",
    "@fp-ts/optic": "^0.1.0",
    "@fp-ts/schema": "^0.1.3",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^8.5.4",
    "immer": "^9.0.19",
    "rc-drawer": "4.4.3",
    "rc-input-number": "^7.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.0",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^5.3.0",
    "superjson": "^1.12.2",
    "use-debounce": "^9.0.3",
    "use-immer": "^0.8.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cssnano": "^5.1.14",
    "cypress": "^11.2.0",
    "esbuild": "^0.15.18",
    "esbuild-register": "^3.4.2",
    "eslint": "8.28.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "react-toastify": "^9.1.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.8"
  },
  "engines": {
    "node": ">=18"
  }
}
